     background-color: #ececec;
     position: absolute;
     text-align: center;
     left: 30rpx;
     top: 0;
 }
 .aui-timeline .aui-timeline-item-label-icon {
     width: 60rpx;
     height: 60rpx;
     font-size: 28rpx;
     line-height: 60rpx;
     background-color: #ececec;
     position: absolute;
     border-radius: 50%;
     text-align: center;
     left: 1.1200rpx;
     top: 0;
 }
 .aui-timeline .aui-timeline-item-inner {
     margin-left: 3.7200rpx;
     margin-right: 30rpx;
     padding: 0;
     position: relative;
 }
 /*基础动画类*/
 @keyframes rotate {
     0% {
         -webkit-transform: rotate(0deg) scale(1);
                 transform: rotate(0deg) scale(1);
     }
     50% {
         -webkit-transform: rotate(180deg) scale(1);
                 transform: rotate(180deg) scale(1);
     }
     100% {
         -webkit-transform: rotate(360deg) scale(1);
                 transform: rotate(360deg) scale(1);
     }
 }
 @-webkit-keyframes rotate {
     0% {
         -webkit-transform: rotate(0deg) scale(1);
                 transform: rotate(0deg) scale(1);
     }
     50% {
         -webkit-transform: rotate(180deg) scale(1);
                 transform: rotate(180deg) scale(1);
     }
     100% {
         -webkit-transform: rotate(360deg) scale(1);
                 transform: rotate(360deg) scale(1);
     }
 }
 @keyframes bounce {
     0%, 100% {
     -webkit-transform: scale(0.0);
             transform: scale(0.0);
     }
     50% {
     -webkit-transform: scale(1.0);
             transform: scale(1.0);
     }
 }
 @-webkit-keyframes bounce {
     0%, 100% {
         -webkit-transform: scale(0.0);
                 transform: scale(0.0);
     }
     50% {
         -webkit-transform: scale(1.0);
                 transform: scale(1.0);
     }
 }
 @keyframes fadeIn {
     from { opacity: 0.3; }
     to { opacity: 1; }
 }
 @-webkit-keyframes fadeIn {
     from { opacity: 0.3; }
     to { opacity: 1; }
 }
