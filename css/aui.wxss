 /*时间轴*/
 .aui-timeline {
     position: relative;
     padding: 0;
     list-style: none;
 }
 .aui-timeline:before {
     content: '';
     position: absolute;
     top: 0;
     left: 1.8200rpx;
     width: 2px;
     height: 100%;
     background: #ececec;
     z-index: 0;
 }
 .aui-timeline .aui-timeline-item {
     position: relative;
     margin-bottom: 30rpx;
 }
 .aui-timeline .aui-timeline-item-header {
     background-color: #ececec;
     padding: 8rpx 20rpx;
     margin: 30rpx;
     text-align: center;
     display: inline-block;
     position: relative;
     z-index: 1;
     font-size: 28rpx;
 }
